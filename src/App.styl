table
  margin: auto
  width: 95vw
  border: 2px solid green
  border-collapse: collapse
  color: green
  margin-bottom: 2vh
  td, th
    border: 1px solid green
  td:nth-child(2)
    height: 10vh
    width: 10vh
  th
    &:hover
      cursor: pointer
    color: darkblue
    background: grey
h1
  text-align: center
  color: red
  font-size: 10vh
.pagContainer
  display: flex
  justify-content: space-around
  .Pagination
  .activePagination
    cursor: pointer
    width: 5vw
    height: 5vw
    color: black
    font-size: 4vw
    border: 2px solid green
    background: cyan
  .activePagination
    background: red
img
  display: block
  width: 100%
  height: 100%
.Loader
  position fixed
  top 0
  left 0
  width: 100vw
  height: 100vh
  background: rgb(yellow);
  z-index: 1000
.loadedLoader
  display: none
.startLoader
  width: 5px
  height: 20px
  background: red
.endLoader
  width: 500px
  height: 20px
  background: red
  transition: width 0.5s
